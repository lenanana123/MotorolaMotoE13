		function normalize_li (data_large_value) {
			var max_height = 44;

			$('[data-large="' + data_large_value + '"]').css('height', 'inherit').each(function(){
				if ($(this).height() > 44) {
					$(this).css('line-height', '25px');

					if ($(this).height() > max_height) {
						max_height = $(this).height();
					}
				}
			}).css('height', max_height);
		}
		
	</script>

	


<script type="text/javascript">

	function findParentId (el, var_id) {

		if (el.id == var_id) {

			return el;
		}

		while (el.parentNode) {

			el = el.parentNode;

			if (el.id == var_id) {

				return el;
			}
		}

		return null;
	}

	setInterval(function(){buildZonasPositions();}, 300);

</script>


